{
  "name": "@rightcapital/phpdoc-parser",
  "version": "0.3.9",
  "keywords": [
    "PHP",
    "PHPDoc",
    "PHPDoc Parser",
    "Parser",
    "TypeScript",
    "PHPStan",
    "AST",
    "Lexer"
  ],
  "description": "TypeScript version of PHPDoc parser with support for intersection types and generics",
  "main": "dist/index.js",
  "repository": "https://github.com/RightCapitalHQ/phpdoc-parser",
  "author": "RightCapital Ecosystem team <npm-publisher@rightcapital.com>",
  "license": "MIT",
  "packageManager": "pnpm@8.7.4",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=16.x",
    "pnpm": ">=8.x"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "7.22.19",
    "@babel/preset-env": "7.22.15",
    "@babel/preset-typescript": "7.22.15",
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@commitlint/cz-commitlint": "17.7.1",
    "@rightcapital/eslint-config-javascript": "7.0.1",
    "@rightcapital/eslint-config-typescript": "7.0.1",
    "@rightcapital/prettier-config": "6.0.1",
    "@types/jest": "29.5.5",
    "@types/lodash": "4.14.198",
    "babel-jest": "29.7.0",
    "beachball": "2.37.0",
    "commitizen": "4.3.0",
    "eslint": "8.49.0",
    "husky": "8.0.3",
    "inquirer": "9.2.11",
    "jest": "29.7.0",
    "prettier": "3.0.3",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@types/node": "18.17.15",
    "lodash": "4.17.21"
  },
  "scripts": {
    "commit": "cz",
    "clean": "tsc --build --clean ./tsconfig.build.json",
    "change": "beachball change --no-commit",
    "check": "beachball check",
    "build": "pnpm run clean && tsc --project ./tsconfig.build.json",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "start": "ts-node src/index.ts",
    "eslint": "eslint --report-unused-disable-directives 'src/**/*.ts*'",
    "eslint:fix": "eslint --report-unused-disable-directives --fix 'src/**/*.ts*'",
    "test": "jest"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
